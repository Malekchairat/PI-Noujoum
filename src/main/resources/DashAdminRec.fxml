<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="754.0" prefWidth="808.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.DashAdminRecController">
    <children>
        <TextField fx:id="searchField" layoutX="63.0" layoutY="31.0" prefHeight="26.0" prefWidth="382.0" promptText="Search here ..." />
        <ListView fx:id="RecListView" layoutX="14.0" layoutY="85.0" prefHeight="277.0" prefWidth="506.0" />
        <BarChart fx:id="barChart" layoutX="533.0" layoutY="414.0" prefHeight="277.0" prefWidth="266.0">
            <xAxis>
                <CategoryAxis side="BOTTOM" />
            </xAxis>
            <yAxis>
                <NumberAxis side="LEFT" />
            </yAxis>
        </BarChart>
        <PieChart fx:id="pieChart" layoutX="533.0" layoutY="85.0" prefHeight="277.0" prefWidth="257.0" />
        <Button fx:id="stats" layoutX="635.0" layoutY="714.0" mnemonicParsing="false" onAction="#openStat" text="Statistiques" />
      <ListView fx:id="RecTreatedListView" layoutX="14.0" layoutY="414.0" prefHeight="323.0" prefWidth="506.0" />
      <Button fx:id="feedback" layoutX="228.0" layoutY="377.0" mnemonicParsing="false" onAction="#openFeedback" prefHeight="25.0" prefWidth="77.0" text="FeedBacks" />
    </children>
</AnchorPane>
